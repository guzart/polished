{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".js$",
        "exclude": ["node_modules", "docs"],
        "excludePattern": ".test.js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "docs_theme/",
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
